<div class="filterJourney">
    <form class="dateFilter" action="/fixture/filter" method="get">
        <input type="hidden" name="tournamentId" value="<%= tournamentId %>">
        <input type="hidden" name="divisionId" value="<%= divisionId %>">
        <div>
            <label for="date">Fechas</label>
            <select name="journey" id="date">
                <% let teams = teamsConsult.length % 2 != 0 ? teamsConsult.length+1-1 : teamsConsult.length-1 %> 
                <% for( let i = 0; i < teams; i++ ) { %>
                    <option <%= locals.journey == i+1 ? "selected" : null  %>  value="<%= i+1 %>"><%= i+1 %></option>
                <% } %>
                <option value="all">Ver todas</option>
            </select>
        </div>
        <div>
            <button class="btn btn--blue" type="submit">FILTRAR FECHA</button>
        </div>
    </form>
</div>